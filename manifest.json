{

   "background": {
      "service_worker": "js/background.js"
   },
   "icons": {
      "16": "img/icon16.png",
      "48": "img/icon48.png",
      "128": "img/icon128.png"
  },
"action": {
    "default_icon": {
      "16": "img/icon16.png",
      "48": "img/icon48.png",
      "128": "img/icon128.png"
    },
    "default_title": "Pi Cast",
    "default_popup": "popup.html"
 },
   "options_page": "options.html",
   "manifest_version": 3,
   "name": "picast",
   "description": "Share videos to Rpi",
   "permissions": [ "contextMenus", "tabs", "notifications" ],
   "version": "1.0.0"
}


